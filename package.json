{
  "name": "kompetisi-indonesia",
  "version": "4.2.0",
  "description": "platform untuk mengikuti dan berbagi kompetisi",
  "main": "dist-server/server/index.js",
  "scripts": {
    "start": "npm run local",
    "local": "concurrently \"npm run server:watch\" \"npm run build:watch\"",
    "build:watch": "NODE_ENV=development webpack --watch --progress",
    "build:prod": "npm run build-client:prod && npm run build-server:prod",
    "lint": "eslint src --ext .js --ext .jsx",
    "test": "jest",
    "test:watch": "jest --watch",
    "server:watch": "NODE_ENV=development nodemon --watch src/ --watch dist-client/ --watch public/ --exec 'node -r dotenv/config -r babel-register' ./src/server",
    "server:prod-test": "NODE_ENV=production nodemon --exec 'node -r dotenv/config' ./dist-server/server",
    "build-client:prod": "rimraf dist-client && NODE_ENV=production APP_KEY='ki-324892374skljfhlsiudfh&6' webpack --progress",
    "build-server:prod": "rimraf dist-server && NODE_ENV=production babel src -d dist-server --ignore '**/*_test.js' --copy-files",
    "release": "standard-version",
    "release:beta": "standard-version -p beta"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/idmore/kompetisiid-web.git"
